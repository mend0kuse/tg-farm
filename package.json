{
    "type": "module",
    "name": "telegram-farm",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "acc:make": "npx tsx ./src/scripts/accounts-generator.ts",
        "acc:update-session": "npx tsx ./src/scripts/update-session.ts",
        "acc:check-session": "npx tsx ./src/scripts/check-session.ts",
        "acc:nicknames": "npx tsx ./src/scripts/update-nicknames.ts",
        "acc:check-wallet": "npx tsx ./src/scripts/check-wallets.ts",
        "acc:randomize-proxies": "npx tsx ./src/scripts/randomize-proxies.ts",
        "launch": "npx tsx ./src/launcher.ts",
        "build": "tsc",
        "start": "node --experimental-specifier-resolution=node dist/launcher.js",
        "lint": "eslint 'src/**/*.{ts,tsx,js}'",
        "lint:fix": "eslint 'src/**/*.{ts,tsx,js}' --fix",
        "format": "prettier --write 'src/**/*.{ts,tsx,js,json,css,md}'",
        "format:check": "prettier --check 'src/**/*.{ts,tsx,js,json,css,md}'"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "dependencies": {
        "@mtcute/convert": "^0.16.9",
        "@mtcute/node": "^0.16.9",
        "@mtcute/socks-proxy": "^0.16.9",
        "@ton-api/client": "0.1.1",
        "@ton-api/ton-adapter": "0.1.0",
        "@ton/core": "0.58.1",
        "@ton/crypto": "3.3.0",
        "@ton/ton": "15.0.0",
        "axios": "^1.7.7",
        "better-sqlite3": "^11.3.0",
        "bip32": "2.0.6",
        "bip39": "3.0.4",
        "bitcoinjs-lib": "6.1.0",
        "bitget-api": "^2.0.10",
        "bybit-api": "^3.10.18",
        "canvas": "^2.11.2",
        "centrifuge": "^5.2.2",
        "crypto": "^1.0.1",
        "dotenv": "^16.4.5",
        "moment-timezone": "^0.5.45",
        "sharp": "^0.33.5",
        "socks-proxy-agent": "^8.0.4",
        "ws": "^8.18.0",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@types/better-sqlite3": "^7.6.11",
        "@types/mongoose": "^5.11.97",
        "@types/node": "^22.5.4",
        "@types/ws": "^8.5.12",
        "@types/xlsx": "^0.0.36",
        "@typescript-eslint/eslint-plugin": "^8.6.0",
        "@typescript-eslint/parser": "^8.6.0",
        "eslint": "^9.11.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "prettier": "^3.3.3",
        "tsx": "^4.19.1",
        "typescript": "^5.6.2"
    }
}
